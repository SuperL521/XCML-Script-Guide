    <!--model
        cmlauncher:config_name="folder_middle"
        cmlauncher:config_value="3" />
    <model
        cmlauncher:config_name="folder_padding"
        cmlauncher:config_value="8" /-->
    <model
        cmlauncher:config_name="icon_crop_padding"
        cmlauncher:config_value="false" />
    <!--     single bg 为只有背板和蒙版时改为true，launcehr中使用程序自带图标也会被处理 -->
    <model
        cmlauncher:config_name="single_bg"
        cmlauncher:config_value="false" />
    <!-- single_wallpaper:壁纸是强制单屏 -->
    <model
        cmlauncher:config_name="single_wallpaper"
        cmlauncher:config_value="true" />
    <!--     folder_bg 为 打开文件夹后显示背景 -->
    <model
        cmlauncher:config_name="folder_bg"
        cmlauncher:config_drawable="wallpaper/folder_bg.jpg" />
